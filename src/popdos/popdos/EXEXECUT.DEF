BEGIN LeftB, MidB, RightB
;

LeftB:		EXECUTE TDIR, TRETURN
MidB:		EXECUTE TDIR, TSPACE, TA, TRETURN
RightB:		EXECUTE TDIR, TSPACE, TB, TRETURN

TDIR:		TYPE "DIR"
TSPACE:		TYPE " "
TA:		TYPE "A:"
TB:		TYPE "B:"
TRETURN:	TYPE ENTER
